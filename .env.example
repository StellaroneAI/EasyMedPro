# Example environment variables for EasyMedPro
# Copy this file to `.env` or `.env.local` and fill in your actual credentials

# API configuration
VITE_API_BASE="https://api.example.com"

# Firebase configuration
VITE_FIREBASE_API_KEY="your_api_key"
VITE_FIREBASE_AUTH_DOMAIN="your_auth_domain"
VITE_FIREBASE_PROJECT_ID="your_project_id"
VITE_FIREBASE_STORAGE_BUCKET="your_storage_bucket"
VITE_FIREBASE_MESSAGING_SENDER_ID="your_sender_id"
VITE_FIREBASE_APP_ID="your_app_id"
VITE_FIREBASE_MEASUREMENT_ID="your_measurement_id"

# Twilio (SMS/OTP) – backend service
VITE_TWILIO_ACCOUNT_SID="your_twilio_account_sid"
VITE_TWILIO_AUTH_TOKEN="your_twilio_auth_token"
VITE_TWILIO_PHONE_NUMBER="your_twilio_phone_number"

# ABHA integration – used for Indian healthcare flows
VITE_ABHA_CLIENT_ID="your_abha_client_id"
VITE_ABHA_CLIENT_SECRET="your_abha_client_secret"

# AI services
VITE_OPENAI_API_KEY="your_openai_api_key"
VITE_AI4BHARAT_API_KEY="your_ai4bharat_api_key" # India-specific

# Backend-only secrets
VITE_MONGODB_URI="your_mongodb_uri"
VITE_JWT_SECRET="your_jwt_secret"
VITE_ENCRYPTION_KEY="your_encryption_key"

# Third-party services
VITE_GOOGLE_MAPS_API_KEY="your_google_maps_api_key"
VITE_FCM_VAPID_KEY="your_fcm_vapid_key"
VITE_FCM_SERVER_KEY="your_fcm_server_key"

