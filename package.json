{
  "name": "easymedpro",
  "private": true,
  "version": "1.0.0",
  "overrides": {
    "react": "18.2.0"
  },
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && cp public/sw.js dist/",
    "preview": "vite preview",
    "firebase:deploy": "npm run build && firebase deploy",
    "firebase:hosting": "npm run build && firebase deploy --only hosting",
    "capacitor:add:android": "npx cap add android",
    "capacitor:add:ios": "npx cap add ios",
    "capacitor:sync": "npx cap sync",
    "capacitor:build:android": "npm run build && npx cap sync && npx cap build android",
    "android:apk": "npm run capacitor:build:android",
    "ios:build": "npm run build && npx cap sync && npx cap build ios",
    "build:firebase": "npm run build && firebase deploy --only hosting",
    "build:android": "npm run build && npx cap sync android && cd android && ./gradlew assembleDebug",
    "build:ios": "npm run build && npx cap sync ios",
    "open:android": "npx cap open android",
    "open:ios": "npx cap open ios",
    "build:all": "npm run build && npx cap sync",
    "dev:android": "npm run build && npx cap sync android && npx cap open android",
    "dev:ios": "npm run build && npx cap sync ios && npx cap open ios",
    "sync": "npx cap sync",
    "sync:android": "npx cap sync android",
    "sync:ios": "npx cap sync ios",
    "deploy:firebase": "npm run build && firebase deploy",
    "serve:firebase": "firebase serve --only hosting",
    "emulate:firebase": "firebase emulators:start",
    "lint": "echo 'No linting configured'",
    "test": "jest"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.2",
    "@capacitor/app": "^7.0.2",
    "@capacitor/browser": "^7.0.2",
    "@capacitor/camera": "^7.0.2",
    "@capacitor/core": "^7.4.2",
    "@capacitor/device": "^7.0.2",
    "@capacitor/dialog": "^7.0.2",
    "@capacitor/filesystem": "^7.1.4",
    "@capacitor/geolocation": "^7.1.5",
    "@capacitor/haptics": "^7.0.2",
    "@capacitor/ios": "^7.4.2",
    "@capacitor/local-notifications": "^7.0.2",
    "@capacitor/network": "^7.0.2",
    "@capacitor/push-notifications": "^7.0.2",
    "@capacitor/share": "^7.0.2",
    "@capacitor-community/speech-recognition": "^4.1.0",
    "@capacitor-community/text-to-speech": "^4.1.3",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-voice/voice": "^3.1.5",
    "@types/node": "^20.14.2",
    "bcryptjs": "^2.4.3",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "expo-av": "^14.0.2",
    "expo-camera": "^14.0.0",
    "expo-file-system": "^16.0.0",
    "expo-media-library": "^15.0.0",
    "expo-speech": "^11.5.0",
    "firebase": "^10.14.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.526.0",
    "mongodb": "^6.18.0",
    "openai": "^5.10.2",
    "rate-limiter-flexible": "^7.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "i18next": "^23.11.4",
    "react-i18next": "^14.1.2",
    "react-native-ble-plx": "^2.0.3",
    "react-native-health": "^1.15.0",
    "twilio": "^5.8.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.2",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-native": "^12.6.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vercel/node": "^3.0.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "firebase-tools": "^13.20.2",
    "jest": "^29.7.0",
    "postcss": "^8.4.38",
    "react-native": "^0.73.6",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "^3.4.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.3.1"
  }
