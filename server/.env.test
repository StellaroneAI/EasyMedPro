# Test Environment Variables for EasyMedPro API Server
NODE_ENV=test
PORT=3001

# CORS Settings
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# MCP Gateway Configuration (test endpoints)
MCP_GATEWAY_BASE_URL=https://test-mcp.stellarone.health
MCP_API_KEY=test-mcp-api-key
MCP_CLIENT_ID=test-client-id
MCP_CLIENT_SECRET=test-client-secret

# Security
JWT_SECRET=test-jwt-secret-key-for-testing-only
ENCRYPTION_KEY=test-encryption-key-32-characters

# Twilio Configuration (test credentials)
TWILIO_ACCOUNT_SID=test-twilio-account-sid
TWILIO_AUTH_TOKEN=test-twilio-auth-token
