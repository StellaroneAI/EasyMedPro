# Firebase Configuration (test project)
FIREBASE_PROJECT_ID=easymed-test
FIREBASE_CLIENT_EMAIL=test@easymed-test.iam.gserviceaccount.com
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nTEST_PRIVATE_KEY\n-----END PRIVATE KEY-----\n"

# Database Configuration (test database)
MONGODB_URI=mongodb://localhost:27017/easymedpro-test

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000

# Logging
LOG_LEVEL=error
DEBUG_TESTS=false
